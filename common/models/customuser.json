{
  "name": "CustomUser",
  "pathLink": "CustomUsers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "crud": {
    "metaFunctions": {},
    "fields": {
      "id": {},
      "username": {
        "title": "user name"
      },
      "email": {
        "title": "email"
      },
      "emailVerified": {
        "type": "Number",
        "editable": "true"
      },
      "mainImageId": {
        "formatter": "related-pattern",
        "pattern": "image",
        "title": "image"
      },
      "published": {
        "type": "datetimePicker",
        "width": "160"
      }
    }
  },
  "properties": {
    "mainImageId": "number",
    "name": {
      "type": "String",
      "required": false,
      "length": 50,
      "precision": null,
      "scale": null,
      "mysql": {
        "columnName": "name",
        "dataType": "varchar",
        "dataLength": 50,
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "N"
      }
    },
    "phone": {
      "type": "String",
      "required": false,
      "length": 50,
      "precision": null,
      "scale": null,
      "mysql": {
        "columnName": "phone",
        "dataType": "varchar",
        "dataLength": 50,
        "dataPrecision": null,
        "dataScale": null,
        "nullable": "N"
      }
    }
  },
  "validations": [],

  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "mainImage": {
      "type": "belongsTo",
      "model": "Images",
      "foreignKey": "mainImageId"
    },
    "users_meetings": {
      "type": "hasMany",
      "model": "users_meetings",
      "foreignKey": "user"
    },
    "meetings": {
      "type": "hasMany",
      "model": "meetings",
      "foreignKey": "user",
      "keyThrough": "meeting",
      "through": "users_meetings"
    },
    "meetingOwner": {
      "type": "hasMany",
      "model": "meetingOwner",
      "foreignKey": "owner"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "extendedLogin"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "authenticate"
    }
  ],
  "methods": {}
}
