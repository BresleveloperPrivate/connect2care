{
  "name": "CustomUser",
  "base": "User",
  "idInjection": true,
  "strict": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {},
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "extendedLogin"
    },{
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW",
      "property": "getUsersList"
    },{
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "getUsersList"
    }
  ],
  "methods": {}
}
