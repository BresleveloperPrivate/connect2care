{
  "_meta": {
    "modules": [
      {
        "name": "fileshandler",
        "path": "../src/modules/fileshandler",
        "routes": "server/routes",
        "mixins": "server/mixins",
        "models": "server/models",
        "github": "https://github.com/carmel-6000/files-handler",
        "fetch-version": "latest",
        "current-version": "1.0.0"
      },
      {
        "name": "dashboard",
        "path": "../src/modules/dashboard",
        "github": "https://github.com/carmel-6000/newdashboard",
        "fetch-version": "latest",
        "current-version": "1.0.0",
        "enabled": true
      },
      {
        "name": "auth",
        "path": "../src/modules/auth",
        "models": "server/models",
        "github": "https://github.com/carmel-6000/carmel-react-auth",
        "fetch-version": "latest",
        "current-version": "1.0.0",
        "enabled": true
      },
      {
        "name": "scripts",
        "path": "../src/modules/scripts",
        "github": "https://github.com/carmel-6000/scripts",
        "fetch-version": "latest",
        "current-version": "1.0.0",
        "enabled": true
      },
      {
        "name": "tools",
        "path": "../src/modules/tools",
        "github": "https://github.com/carmel-6000/carmel-tools",
        "fetch-version": "latest",
        "current-version": "1.0.0",
        "enabled": true
      },
      {
        "name": "samples",
        "path": "../src/modules/samples",
        "github": "https://github.com/carmel-6000/pumbasamples",
        "fetch-version": "latest",
        "current-version": "1.0.0",
        "enabled": false
      }
    ],
    "sources": [
      "loopback/server/models",
      "../src/modules/auth",
      "../common/models",
      "./models",
      "../src/modules/fileshandler/server/models",
      "../src/modules/auth/server/models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../src/modules/fileshandler/server/mixins"
    ]
  },
  "User": {
    "dataSource": "msql",
    "options": {
      "emailVerificationRequired": true
    }
  },
  "CustomUser": {
    "dataSource": "msql",
    "public": true
  },
  "AccessToken": {
    "dataSource": "msql",
    "public": false
  },
  "Email": {
    "dataSource": "myEmailDataSource"
  },
  "Games": {
    "dataSource": "msql",
    "public": true
  },
  "GamesImages": {
    "dataSource": "msql",
    "public": true
  },
  "Images": {
    "dataSource": "msql",
    "public": true
  },
  "Files": {
    "dataSource": "msql",
    "public": true
  },
  "BaseFiles": {
    "public": false,
    "dataSource": "msql"
  },
  "BaseImages": {
    "public": false,
    "dataSource": "msql"
  },
  "RecordsPermissions": {
    "public": true,
    "dataSource": "msql"
  },
  "ACL": {
    "public": false,
    "dataSource": "msql"
  },
  "RoleMapping": {
    "dataSource": "msql"
  },
  "Role": {
    "public": false,
    "dataSource": "msql"
  },
  "BaseAudios": {
    "public": false,
    "dataSource": "msql"
  },
  "BaseVideos": {
    "public": false,
    "dataSource": "msql"
  }
}
