{
  "_meta": {
    "modules": [
      {
        "name": "fileshandler",
        "path": "../src/modules/fileshandler",
        "routes": "server/routes",
        "mixins": "server/mixins",
        "models": "server/models"
      },
      {
        "name": "auth",
        "path": "../src/modules/auth",
        "models": "server/models"
      }
    ],
    "sources": [
      "loopback/server/models",
      "../src/modules/auth",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../src/modules/fileshandler/server/mixins"
    ]
  },
  "User": {
    "dataSource": "msql",
    "options": {
      "emailVerificationRequired": true
    }
  },
  "CustomUser": {
    "dataSource": "msql",
    "public": true
  },
  "AccessToken": {
    "dataSource": "msql",
    "public": false
  },
  "Email": {
    "dataSource": "myEmailDataSource"
  },
  "Games": {
    "dataSource": "msql",
    "public": true
  },
  "Images": {
    "dataSource": "msql",
    "public": true
  },
  "Files": {
    "dataSource": "msql",
    "public": true
  }
}